{
    "name": "elephox/web",
    "description": "Elephox Web library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "support": {
        "issues": "https://github.com/elephox-dev/framework/issues",
        "source": "https://github.com/elephox-dev/framework"
    },
    "require": {
        "php": "^8.1 <8.3",
        "ext-mbstring": "*",
        "elephox/autoloading": "dev-develop",
        "elephox/collection": "dev-develop",
        "elephox/configuration": "dev-develop",
        "elephox/console": "dev-develop",
        "elephox/di": "dev-develop",
        "elephox/files": "dev-develop",
        "elephox/http": "dev-develop",
        "elephox/logging": "dev-develop",
        "elephox/oor": "dev-develop",
        "elephox/support": "dev-develop",
        "jetbrains/phpstorm-attributes": "^1.0",
        "symfony/process": "^6.0"
    },
    "provide": {
        "psr/log-implementation": "3.0"
    },
    "suggest": {
        "psr/log": "If you want to use logging."
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Web\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Web\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
