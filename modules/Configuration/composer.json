{
    "name": "elephox/configuration",
    "description": "Elephox configuration library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 <8.3",
        "ext-json": "*",
        "elephox/collection": "^0.4",
        "elephox/files": "^0.4",
        "elephox/oor": "^0.4",
        "elephox/support": "^0.4",
        "jetbrains/phpstorm-attributes": "^1.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Configuration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Configuration\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "^0.4": "0.4.x-dev",
            "dev-main": "1.x-dev"
        }
    }
}
