{
    "name": "elephox/console",
    "description": "Elephox console library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 <8.3",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "elephox/autoloading": "dev-develop",
        "elephox/collection": "dev-develop",
        "elephox/configuration": "dev-develop",
        "elephox/di": "dev-develop",
        "elephox/logging": "dev-develop",
        "elephox/support": "dev-develop",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "suggest": {
        "filp/whoops": "To be able to use the Whoops error handler ($builder->addWhoops())",
        "nunomaduro/collision": "For pretty formatting of exceptions in command line usage"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Console\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
