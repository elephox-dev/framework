{
    "name": "elephox/support",
    "description": "Elephox support library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2"
    },
    "suggest": {
        "ext-json": "To use Elephox\\JsonConvertible"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Support\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
