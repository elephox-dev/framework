{
    "name": "elephox/support",
    "description": "Elephox support library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "support": {
        "issues": "https://github.com/elephox-dev/framework/issues",
        "source": "https://github.com/elephox-dev/framework"
    },
    "require": {
        "php": "^8.1 <8.3",
        "elephox/mimey": "^4.0",
        "elephox/oor": "^0.5"
    },
    "suggest": {
        "ext-fileinfo": "To get the mime type of a file"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Support\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
