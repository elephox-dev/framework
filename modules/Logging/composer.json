{
    "name": "elephox/logging",
    "description": "Elephox Logging library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 <8.3",
        "jetbrains/phpstorm-attributes": "^1.0",
        "psr/log": "^3.0",
        "ricardoboss/php-console-logger": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Logging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Logging\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
