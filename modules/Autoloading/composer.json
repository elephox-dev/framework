{
    "name": "elephox/autoloading",
    "description": "Elephox auto loading library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "support": {
        "issues": "https://github.com/elephox-dev/framework/issues",
        "source": "https://github.com/elephox-dev/framework"
    },
    "require": {
        "php": "^8.1 <8.3",
        "elephox/collection": "^0.6",
        "elephox/files": "^0.6",
        "elephox/oor": "^0.6",
        "jetbrains/phpstorm-attributes": "^1.0",
        "psr/container": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Autoloading\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Autoloading\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
