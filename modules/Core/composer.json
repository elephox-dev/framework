{
    "name": "elephox/core",
    "description": "Elephox core library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "elephox/collection": "dev-main",
        "elephox/di": "dev-main",
        "elephox/events": "dev-main",
        "elephox/files": "dev-main",
        "elephox/http": "dev-main",
        "elephox/oor": "dev-main",
        "elephox/stream": "dev-main",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Core\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
