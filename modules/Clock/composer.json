{
    "name": "elephox/clock",
    "description": "Elephox clock library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "support": {
        "issues": "https://github.com/elephox-dev/framework/issues",
        "source": "https://github.com/elephox-dev/framework"
    },
    "require": {
        "php": "^8.1 <8.3",
        "jetbrains/phpstorm-attributes": "^1.0",
        "stella-maris/clock": "^0.1"
    },
    "provide": {
        "psr/clock-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Clock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Clock\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}