{
    "name": "elephox/http",
    "description": "Elephox HTTP library.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-mbstring": "*",
        "elephox/collection": "^0.0",
        "elephox/oor": "^0.0",
        "elephox/stream": "^0.0",
        "elephox/support": "^0.0",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Http\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
