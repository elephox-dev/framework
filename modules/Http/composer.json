{
    "name": "elephox/http",
    "description": "Elephox HTTP library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "support": {
        "issues": "https://github.com/elephox-dev/framework/issues",
        "source": "https://github.com/elephox-dev/framework"
    },
    "require": {
        "php": "^8.2 <8.4",
        "ext-mbstring": "*",
        "elephox/collection": "dev-develop",
        "elephox/files": "dev-develop",
        "elephox/mimey": "^4.0",
        "elephox/oor": "dev-develop",
        "elephox/stream": "dev-develop",
        "elephox/support": "dev-develop",
        "jetbrains/phpstorm-attributes": "^1.0",
        "psr/http-message": "^1.1"
    },
    "require-dev": {
        "nyholm/psr7": "^1.5"
    },
    "provide": {
        "psr/http-message-implementation": "1.1"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Http\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
