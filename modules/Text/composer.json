{
    "name": "elephox/text",
    "description": "Elephox Text library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2",
        "elephox/collection": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Text\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Text\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
