{
    "name": "elephox/database",
    "description": "Elephox database library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-mysqli": "*",
        "elephox/collection": "dev-main",
        "elephox/di": "dev-main",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\Database\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    }
}
