{
    "name": "elephox/oor",
    "description": "Elephox Object-Oriented Revolution library.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricaro Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "elephox/collection": "^0.3 || ^1.0"
    },
    "autoload": {
        "psr-4": {
            "Elephox\\OOR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephox\\OOR\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "elephox/composer-module-sync": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev",
            "dev-main": "1.x-dev"
        }
    }
}
